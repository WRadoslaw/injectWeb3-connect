{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "CommonJS",
    "target": "es5",
    "allowJs": true,
    "strict":true,
    "strictNullChecks":true,
    "alwaysStrict":true,
    "noUnusedLocals":true,
    "noUnusedParameters":true,
    "noImplicitReturns":true,
    "forceConsistentCasingInFileNames":true,
    "noFallthroughCasesInSwitch":true,
    "allowUnreachableCode":false
  },
  "files": ["src/index.ts"]
}
